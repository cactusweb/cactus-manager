{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "mongo": true,
    "es6": true
  },
  "extends": [
    "airbnb",
    "prettier",
    "plugin:node/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "semi": "off",
    "no-underscore-dangle": "off",
    "import/extensions": "off",
    "no-console": "off",
    "import/prefer-default-export": "off",
    "consistent-return": "off",
    "no-return-await": "off"
  }
}